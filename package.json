{
  "name": "medux-wechat-demo",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev-tools": "remotedev --hostname=localhost --port=8000",
    "dev-server": "node ./devServer/start.js",
    "compile": "./node_modules/typescript/bin/tsc",
    "stylelint": "stylelint \"./src/**/*.less\"",
    "eslint": "eslint \"./src/**/*.ts\"",
    "tsc": "tsc",
    "babel": "babel ./src --out-dir ./src  --extensions \".ts\"",
    "babel-watch": "babel ./src --watch --out-dir ./src  --extensions \".ts\"",
    "build": "npm run eslint && npm run stylelint && npm run tsc && npm run babel",
    "start": "npm run eslint && npm run stylelint && npm run tsc && concurrently \"npm:babel-watch\" \"npm:dev-server\"",
    "push": "scp -r devServer/* root@127.0.0.1:/var/www/ztty-wechat"
  },
  "keywords": [],
  "author": {
    "name": "wooline",
    "email": "wooline@qq.com"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "~7.10.3",
    "@babel/core": "~7.10.3",
    "@babel/plugin-proposal-class-properties": "~7.10.1",
    "@babel/plugin-proposal-decorators": "~7.10.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "~7.10.3",
    "@babel/plugin-proposal-optional-chaining": "~7.10.3",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.10.3",
    "@babel/preset-env": "~7.10.3",
    "@babel/preset-typescript": "~7.10.1",
    "@medux/dev-utils": "1.0.8",
    "@typescript-eslint/eslint-plugin": "~3.4.0",
    "@typescript-eslint/parser": "~3.4.0",
    "babel-plugin-root-import": "~6.5.0",
    "body-parser": "1.20.1",
    "chalk": "4.1.0",
    "concurrently": "~5.2.0",
    "cookie-parser": "1.4.5",
    "eslint": "~7.3.1",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-prettier": "~3.1.4",
    "express": "4.18.2",
    "http-proxy-middleware": "1.0.4",
    "json-format": "1.0.1",
    "miniprogram-api-typings": "~2.11.0",
    "mockjs": "1.1.0",
    "prettier": "~2.0.2",
    "remotedev-server": "~0.3.1",
    "stylelint": "~13.6.1",
    "stylelint-config-prettier": "~8.0.2",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "~2.3.0",
    "stylelint-prettier": "~1.1.1",
    "typescript": "~3.8.3"
  }
}
